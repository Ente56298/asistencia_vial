{
  "name": "asistencia-vial-mexico",
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_GEMINI_API_KEY": "@vite_gemini_api_key"
  },
  "build": {
    "env": {
      "VITE_GEMINI_API_KEY": "@vite_gemini_api_key"
    }
  },
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}